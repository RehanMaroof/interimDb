{
    "version": 2,
    "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/routes/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/controllers/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/models/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/services/**/*.js",
      "use": "@vercel/node"
    }
  ],
    "routes": [
    {
      "src": "/api/customers/(.*)",
      "dest": "/src/routes/customerRoutes.js"
    },
    {
      "src": "/api/jobs/(.*)",
      "dest": "/src/routes/jobRoutes.js"
    },
    {
      "src": "/api/test/(.*)",
      "dest": "/src/routes/leapTestRoutes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}